{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
	"parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-base",
    "airbnb-base-typescript"
  ],
  "parserOptions": {
    "project": "**/tsconfig.json"
  },
  "rules": {
    "import/no-import-module-exports": "off"
  },
  "plugins": ["typescript"]
}
